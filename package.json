{
  "name": "ease-audio",
  "version": "0.0.21",
  "description": "The Audio Player Project",
  "keywords": [
    "ease",
    "ease-audio",
    "ease-audio.js",
    "audio",
    "sound",
    "web audio",
    "webaudio",
    "html5",
    "html5 audio"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack-dashboard -p 3300 -- node demo/server.js",
    "static": "webpack --config build/webpack.config.js --mode development --watch",
